{
  "framework": "nextjs",
  "buildCommand": "npm run build:vercel",
  "installCommand": "npm install",
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "SKIP_ENV_VALIDATION": "1",
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "1",
      "NODE_ENV": "production"
    }
  },
  "outputDirectory": ".next"
}
